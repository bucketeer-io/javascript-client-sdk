  "devDependencies": {
    "@types/jsdom": "20.0.1",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "eslint": "8.31.0",
    "jsdom": "21.0.0",
    "typescript": "4.9.4",
    "vite": "4.0.4",
    "vitest": "0.27.1"
  },
  "packageManager": "pnpm@7.23.0"
