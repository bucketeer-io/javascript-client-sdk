  "devDependencies": {
    "@types/jsdom": "20.0.1",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "eslint": "8.33.0",
    "jsdom": "21.1.0",
    "typescript": "4.9.4",
    "vite": "4.0.4",
    "vitest": "0.28.3"
  },
  "packageManager": "pnpm@7.26.2"
