{
  "name": "@bucketeer/js-client-sdk",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.0",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/bucketeer.umd.cjs",
  "module": "./dist/bucketeer.js",
  "types": "./dist/types/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/bucketeer.js",
      "require": "./dist/bucketeer.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build && tsc --emitDeclarationOnly --incremental false --composite false --project tsconfig.lib.json",
    "preview": "vite preview",
    "test": "vitest --config ./vitest.config.ts --dir test",
    "test:e2e": "vitest --config ./vitest-e2e.config.ts --dir e2e",
    "lint": "eslint --ext .ts,.js,.cjs .",
    "lint:fix": "eslint --ext .ts,.js,.cjs --fix .",
    "example:serve": "pnpm build && pnpm --filter example serve"
  },
  "devDependencies": {
    "@types/jsdom": "21.1.1",
    "@types/node": "18.16.6",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "@vitest/browser": "0.29.7",
    "cross-fetch": "3.1.5",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "happy-dom": "9.10.9",
    "jiti": "1.18.2",
    "msw": "1.2.1",
    "prettier": "2.8.8",
    "typescript": "5.0.4",
    "vite": "4.3.5",
    "vitest": "0.29.7",
    "webdriverio": "8.6.9"
  },
  "packageManager": "pnpm@8.4.0"
}
